{
  "comment" : "This is bundled JSON file. Idea is that we should externalize this also as part of build. Currently this is going to hold the dev env information",
  "http.port" : 8080,
  "max.file.size.bytes" : 5242880,
  "upload.location" : "/mnt/nfs/repository/",
  "s3.config": {
      "s3.access.key" : "[ACCESS_KEY]",
      "s3.secret" : "[SECRET_KEY]",
      "s3.user.bucket.name" : "[USER_BUCKET_NAME]",
      "s3.content.bucket.name" : "[CONTENT_BUCKET_NAME]"
  },
  "redis.config" : {
    "host" : "localhost",
    "port" : 6379,
    "encoding" : "UTF-8",
    "tcpKeepAlive" : true,
    "tcpNoDelay" : true
  }
}
